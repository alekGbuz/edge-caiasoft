{
  "mappings" : [
    {
      "request": {
        "method": "POST",
        "url": "/authn/login",

        "headers" : {
          "x-okapi-tenant": {
            "equalTo": "test_tenant"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "x-okapi-token": "test_token"
        }
      }
    }
  ]
}
